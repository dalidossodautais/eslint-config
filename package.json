{
  "author": "dalidossodautais",
  "description": "ESLint configuration from Pro/website project with custom rules",
  "devDependencies": {
    "@commitlint/cli": "19.0.0",
    "@commitlint/config-conventional": "19.0.0",
    "@commitlint/types": "19.0.0",
    "@eslint/js": "9.37.0",
    "@eslint/json": "0.14.0",
    "@eslint/markdown": "7.5.1",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "@typescript-eslint/types": "8.45.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-package-json": "0.56.3",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2",
    "typescript": "5.9.3"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "rules/"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "react",
    "nextjs"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,mjs,ts,tsx,json,md,ts}": [
      "eslint --max-warnings 0"
    ]
  },
  "main": "./dist/index.js",
  "name": "@dalidossodautais/eslint-config",
  "peerDependencies": {
    "@eslint/js": "9.37.0",
    "@eslint/json": "0.14.0",
    "@eslint/markdown": "7.5.1",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-package-json": "0.56.3",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.4.0"
  },
  "scripts": {
    "build": "tsc",
    "deploy:major": "pnpm version major && pnpm build && pnpm publish --access public && git push --follow-tags",
    "deploy:minor": "pnpm version minor && pnpm build && pnpm publish --access public && git push --follow-tags",
    "deploy:patch": "pnpm version patch && pnpm build && pnpm publish --access public && git push --follow-tags",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.12"
}
